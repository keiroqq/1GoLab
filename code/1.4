package main

import "fmt"

func main() {
	firstDigit := 10
	secondDigit := 4

	fmt.Println("Multiplication: ", firstDigit*secondDigit,
		"\nAddition: ", firstDigit+secondDigit,
		"\nSubtraction: ", firstDigit-secondDigit,
		"\nDivision: ", firstDigit/secondDigit,
		"\nRemainder: ", firstDigit%secondDigit)
}
